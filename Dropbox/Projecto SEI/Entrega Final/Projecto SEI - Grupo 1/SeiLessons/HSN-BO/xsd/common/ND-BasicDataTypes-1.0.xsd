	<xsd:simpleType name="signatureHashSubsetType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Caracteres correspondentes à 1º, 11º, 21ª e 31ª do hash da assinatura</xsd:documentation>
		</xsd:annotation>
