	</xsd:complexType>
	<xsd:complexType name="documentStatusInformationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Informação de estado de documentos</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="status" type="documentStatusEnumType">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Código de estado do documento.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="documentError" type="documentErrorType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Código de erro e sua descrição</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="documentType">
		<xsd:annotation>
			<xsd:documentation source="pt">Tipo de dados da qual derivam todos os documentos. Herda do baseDocumentType, namespace urn:netdocs:schemas:message, para que todos os documentos possam ser enviados dentro do elemento message</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msg:baseDocumentType">
				<xsd:sequence>
					<xsd:element name="extensions" type="extensionType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Extenções ao documento. Pode ser inserido aqui qualquer estrutura XML válida. Permite o envio de informação não suportada pelo schema.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="binaryDocumentFormat" type="binaryDocumentType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Para enviar o documento em formatos binários.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="binaryDocumentAttachment" type="binaryAttachmentType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Para enviar anexos ao documento, em formatos binários.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="documentLocation" type="documentLocationType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Localização do documento em sistemas externos</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attributeGroup ref="documentAttributesGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="documentWithBaseEntities">
		<xsd:complexContent>
			<xsd:extension base="documentType">
				<xsd:sequence>
					<xsd:element name="seller" type="nonBillableCompanyInformationType">
						<xsd:annotation>
							<xsd:documentation>Entidade vendedora ou fornecedora dos artigos/serviços especificados no documento</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="buyer" type="nonBillableCompanyInformationType">
						<xsd:annotation>
							<xsd:documentation>Entidade compradora dos artigos/serviços especificados no documento</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="billTo" type="nonBillableCompanyInformationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Entidade a ser facturada.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="shipTo" type="nonBillableCompanyInformationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Entidade/localização para onde se deverá enviar a mercadoria.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="documentWithBaseEntitiesHeader">
		<xsd:complexContent>
			<xsd:extension base="documentWithBaseEntities">
				<xsd:sequence>
					<xsd:element name="additionalDate" type="documentAdditionalDateType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Informação de tipo data e hora presentes no documento, com exepção da data do documento. Por exemplo, data de entrega.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="reference" type="documentReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Referências a outros documentos.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transportInformation" type="transportInformationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Dados relativos ao transporte das mercadorias</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="currencyCode" type="currencyCodeType">
						<xsd:annotation>
							<xsd:documentation>Código da moeda ISO 4217 Alpha 3.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="discount" type="discountInformationType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Descontos globais ao documento</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="charge" type="chargeInformationType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Encargos globais ao documento</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="comment" type="structuredDescriptionType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Comentários e observações globais ao documento.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="documentWithBaseEntitiesHeaderBody">
		<xsd:complexContent>
			<xsd:extension base="documentWithBaseEntitiesHeader">
				<xsd:sequence>
					<xsd:element name="lineItem" type="documentLineItemType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Detalhe das linhas de artigo ou serviços</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="documentWithBaseEntitiesHeaderBodySummary">
		<xsd:complexContent>
			<xsd:extension base="documentWithBaseEntitiesHeaderBody">
				<xsd:sequence>
					<xsd:element name="totalLineItemNetAmount" type="nonNegativeDecimalType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Somatório dos montantes líquidos das linhas</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="totalLineItemGrossAmount" type="nonNegativeDecimalType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Somatório dos montantes brutos das linhas</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="totalDiscountAmount" type="nonNegativeDecimalType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Somatório dos montantes de descontos das linhas</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="totalChargeAmount" type="nonNegativeDecimalType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="pt">Somatório dos montantes de encargos das linhas</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="consumerContractType">
		<xsd:annotation>
			<xsd:documentation>B2C consumer contract type</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="biller" type="companyInformationType">
				<xsd:annotation>
					<xsd:documentation>Dados relatiivos à entidade que irá enviar facturas para este consumidor ao abrigo deste contrato</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contractId">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Identificação do contracto/conta deste consumidor perante o biller</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="nonEmptyString">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="contractLocation" type="consumerContractLocationType">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Dados relativos ao local de consumo</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="consumerContractLocationType">
		<xsd:annotation>
			<xsd:documentation>B2C consumer contract location type</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="locationId">
				<xsd:annotation>
					<xsd:documentation>Código de identificação do local de consumo</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="nonEmptyString">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="locationAddress" type="addressInformationType">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Morada do local de consumo</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
