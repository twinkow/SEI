<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:annotation>
		<xsd:documentation xml:lang="pt">
			Módulo para conter os tipos de dados partilhados entre diferentes namespaces.
			Não está qualificado com namespace para poder ser integrado em outros namespaces, sem ter de se criar prefixos adicionais.
		</xsd:documentation>
	</xsd:annotation>
	<!-- ======================== -->
	<!-- =====   Simple Types  ====== -->
	<!-- ======================== -->
	<xsd:simpleType name="nonNegativeDecimalType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Número decimal positivo, com valor mínimo de 0.000</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0.000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="nonNegativeIntegerType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Número inteiro positivo, com valor mínimo de 0</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="base64Type">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para conter informação codificada em base64. Normalmente utiliza-se para transmitir informação binária, que deve ser previamente codificada em base64.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:base64Binary">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="contentTypeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados para especificar o tipo de conteúdo de um ficheiro. O schema só verifica o formato, não verifica se se trata de um tipo válido ou não. Derão ser usados Content Types válidos. Exemplos válidos: text/plain application/pdf image/jpeg .</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:pattern value="[a-z]+/[a-z\.\-\+]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="countryCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Código de país. Deve-se colocar aqui um código ISO 3166-1 alpha 2 válido.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="countryCodeTypeEnum"/>
	</xsd:simpleType>
	<xsd:simpleType name="cityType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Nome de cidade</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="nonEmptyString">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="postCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Código postal suportando os seguintes formatos: 
		#####
		####
		####-###
		##-###
		X-####
		XX## #XX
		XX# #XX
		X#X #X#
		(#=número; X=Letra)
		UK postcode
		NL postcode</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="zip">
				<xsd:simpleType>
					<xsd:restriction base="nonEmptyString">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="10"/>
						<xsd:pattern value="([0-9]{2}\-[0-9]{3}|[0-9]{5}|[0-9]{4}|[0-9]{4}\-[0-9]{3}|(\d{4} [A-Z]{2})|([A-Z]{2}[0-9]{2})|([A-Z]{1}\d{1}[A-Z]{1} \d{1}[A-Z]{1}\d{1})|([0-9]{1}[A-Z]{2})|(\d{4} [A-Z]{2} \d{2})|((([A-PR-UWYZ][0-9][0-9A-HJKS-UW]?)|([A-PR-UWYZ][A-HK-Y][0-9][0-9ABEHMNPRV-Y]?))\s{0,2}[0-9]([ABD-HJLNP-UW-Z]{2}))|(GIR\s{0,2}0AA))"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="area">
				<xsd:simpleType>
					<xsd:restriction base="nonEmptyString">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="phoneContactType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Número de telefone</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="nonEmptyString">
			<xsd:maxLength value="20"/>
			<xsd:pattern value="((\+\d{1,3}(-| )?\(?\d\)?(-| )?\d{1,5})|(\(?\d{2,6}\)?))(-| )?(\d{3,4})(-| )?(\d{4})(( x| ext)\d{1,5}){0,1}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="emailContactType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Formato para endereços de email</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="nonEmptyString">
			<xsd:maxLength value="50"/>
			<xsd:pattern value="([a-zA-Z0-9_\-\.])+@(([0-2]?[0-5]?[0-5]\.[0-2]?[0-5]?[0-5]\.[0-2]?[0-5]?[0-5]\.[0-2]?[0-5]?[0-5])|((([a-zA-Z0-9\-])+\.)+([a-zA-Z\-])+))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="addressType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Morada</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="nonEmptyString">
			<xsd:maxLength value="200"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="entityNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Nome de entidade</xsd:documentation>
