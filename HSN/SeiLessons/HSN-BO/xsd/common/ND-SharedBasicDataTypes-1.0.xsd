			<xsd:enumeration value="BUYERASSIGNED">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Identificador atribuido pelo comprador</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SELLERASSIGNED">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Identificador atribuido pelo comprador</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAT">
