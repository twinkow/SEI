				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Factura comercial</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CREDITNOTE">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Nota de crédito</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DEBITNOTE">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Nota de débito</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ORDER">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Encomenda</xsd:documentation>
				</xsd:annotation>
