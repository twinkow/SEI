	<xsd:simpleType name="sharedDocumentInAttachEnumType">
		<xsd:annotation>
			<xsd:documentation>Tipos de documento que podem ser anexados a documentos principais</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sharedDocumentEnumType">
			<xsd:enumeration value="VOUCHER">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Voucher</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ATTACH">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Anexo ao documento</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ======================== -->
	<!-- ===== Complex Types ====== -->
	<!-- ======================== -->
	<xsd:complexType name="entityIdType">
		<xsd:annotation>
			<xsd:documentation>Identificador de entidades.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="nonEmptyString">
				<xsd:attribute name="entityIdTypeCoded" type="entityIdTypeEnumType" use="required">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">Tipo de identificador da entidade</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="extensionType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para criar pontos de extensão.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="binaryAttachmentType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para conter anexos binários. O conteúdo deste elemento deve estar codificado em Base64</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="base64Type">
				<xsd:attribute name="name" type="descriptionType">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">O nome do ficheiro.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="contentType" type="contentTypeType" use="required">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">O tipo de conteúdo segundo as definições de MIME. Identifica o formato do ficheiro binário anexado. Exemplos válidos: text/plain application/pdf image/jpeg </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="documentNumber" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Número do documento anexado, caso se aplique</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="documentTypeCode" type="sharedDocumentInAttachEnumType">
					<xsd:annotation>
						<xsd:documentation>Identificação do tipo do documento anexado.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="binaryDocumentType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para conter anexos binários. O conteúdo deste elemento deve estar codificado em Base64</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="base64Type">
				<xsd:attribute name="name" type="descriptionType">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">O nome do ficheiro.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="contentType" type="contentTypeType" use="required">
					<xsd:annotation>
						<xsd:documentation xml:lang="pt">O tipo de conteúdo segundo as definições de MIME. Identifica o formato do ficheiro binário anexado. Exemplos válidos: text/plain application/pdf image/jpeg </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="addressInformationType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para moradas completas.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="address" type="addressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Rua/Avenida</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="city" type="cityType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nome da cidade.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="postalCode" type="postCodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Código postal.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="countryCode" type="countryCodeType">
				<xsd:annotation>
					<xsd:documentation>Código do país segundo ISO 3166-1 alpha 2</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="contactInformationType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para contactos.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="phone" type="phoneContactType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Número de telefone</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fax" type="phoneContactType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Número de fax.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="email" type="emailContactType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Endereço de email.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="companyInformationType">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados para dados de empresa/parceiro.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="entityIdType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Identificação da empresa.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="entityNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nome da empresa</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="addressInformation" type="addressInformationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Morada completa</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contactInformation" type="contactInformationType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Contactos.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="documentLocationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Informação de localização do documento</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="locationURL" type="xsd:anyURI" nillable="false"/>
			<xsd:element name="description" type="nonEmptyString" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="baseDocumentType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Tipo de dados de onde derivam todos os tipos de documentos. Não pode ser instânciado directamente.</xsd:documentation>
		</xsd:annotation>
	</xsd:complexType>
</xsd:schema>
