				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Outro tipo de identificação</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="URN">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Uniform Resource Name</xsd:documentation>
					<xsd:documentation xml:lang="en">Uniform Resource Name</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NETDOCSID">
				<xsd:annotation>
					<xsd:documentation xml:lang="pt">Identificador interno do Netdocs</xsd:documentation>
					<xsd:documentation xml:lang="en">Netdocs internal identification</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
